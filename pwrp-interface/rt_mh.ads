pragma Ada_2005;
pragma Style_Checks (Off);

with Interfaces.C; use Interfaces.C;
with pwr;

package rt_mh is
   
   
   type mh_uEventInfo;
   type anon_31 is 
      record
	 Status : aliased pwr.pwr_tUInt16;  -- rt_mh.h:59
	 Prio   : aliased pwr.pwr_tUInt16;  -- rt_mh.h:59
      end record;
   pragma Convention (C_Pass_By_Copy, anon_31);
   type mh_uEventInfo (discr : unsigned := 0) is record
      case discr is
         when 0      =>
            c_All : aliased pwr.pwr_tUInt32;  -- rt_mh.h:58
         when others =>
            Event : aliased anon_31;  -- rt_mh.h:62
      end case;
   end record;
   pragma Convention (C_Pass_By_Copy, mh_uEventInfo);
   pragma Unchecked_Union (mh_uEventInfo);  -- rt_mh.h:63


end rt_mh;
